<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mybatis.IBlogMapper">
    <insert id="saveBlog" parameterType="BlogInfo">
        insert into blog(updatetime,updateuser,user,title,content) values(#{updatetime},#{updateuser},#{user},#{title},#{content})
    </insert>
</mapper>