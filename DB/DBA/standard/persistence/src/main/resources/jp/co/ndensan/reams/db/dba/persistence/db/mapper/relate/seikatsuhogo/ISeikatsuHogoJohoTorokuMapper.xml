<mapper namespace="jp.co.ndensan.reams.db.dba.persistence.db.mapper.relate.seikatsuhogo.ISeikatsuHogoJohoTorokuMapper">
    <select resultOrdered="true" id="get生活保護情報登録リスト"  resultMap="SeikatsuJohoEntityResult"
        parameterType="jp.co.ndensan.reams.db.dba.definition.mybatisprm.seikatsuHogoJoho.SeikatsuHogoJohoTorokuMyBatisParameter" fetchSize="500">
        <include refid="selectSeikatsuJohoEntityList" />
    </select>
    <resultMap id="SeikatsuJohoEntityResult" type="jp.co.ndensan.reams.db.dba.entity.db.relate.seikatsuHogoJoho.SeikatsuHogoJohoTorokuEntity" autoMapping="true">
        <association property="生活保護受給者" resultMap="resultMap_seikatsu"/>
        <collection property="扶助種別リスト" resultMap="resultMap_fujyolist"/>
        <collection property="停止期間リスト" resultMap="resultMap_teshilist"/>
    </resultMap>
    <sql id="selectSeikatsuJohoEntityList">
        SELECT
        "UrT0508".*,
        "UrT0528".*,
        "UrT0526".*
        FROM
        rgur."UrT0508SeikatsuHogoJukyusha" AS "UrT0508"
        LEFT OUTER JOIN
        rgur."UrT0528SeikatsuHogoTeishikikan" AS "UrT0528" 
        ON
        "UrT0508"."shikibetsuCode" = "UrT0528"."shikibetsuCode"
        AND  "UrT0508"."gyomuCode" = "UrT0528"."gyomuCode"
        AND  "UrT0508"."jukyuKaishiYMD" = "UrT0528"."jukyuKaishiYMD"
        LEFT OUTER JOIN
        rgur."UrT0526SeikatsuHogoFujoShurui" AS "UrT0526" 
        ON
        "UrT0508"."shikibetsuCode" = "UrT0526"."shikibetsuCode"
        AND  "UrT0508"."gyomuCode" = "UrT0526"."gyomuCode"
        AND  "UrT0508"."jukyuKaishiYMD" = "UrT0526"."jukyuKaishiYMD" 
        WHERE
        "UrT0508"."shikibetsuCode" = #{shikibetsuCode}
        AND  "UrT0508"."gyomuCode" = #{gyomuCode}
        <if test="jukyushaFalg">
        AND  "UrT0508"."jukyushaNo" = #{jukyushaNo}
        </if>
    </sql>
