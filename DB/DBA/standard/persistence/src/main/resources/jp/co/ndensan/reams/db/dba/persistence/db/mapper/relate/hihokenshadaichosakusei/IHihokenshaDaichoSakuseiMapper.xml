        DbT1001."idoYMD" AS "dbT1001_idoYMD",
        DbT1001."edaNo" AS "dbT1001_edaNo",										
        DbT1001."idoJiyuCode" AS "dbT1001_idoJiyuCode",
        DbT1001."shichosonCode" AS "dbT1001_shichosonCode",
