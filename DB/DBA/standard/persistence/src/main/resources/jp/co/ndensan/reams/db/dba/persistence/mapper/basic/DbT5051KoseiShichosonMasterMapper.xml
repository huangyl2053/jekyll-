<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 概要： DbT1001HihokenshaDaichoマスタとエンティティのマッピング用XMLです。 -->
<!-- 作者： n1070 久保裕是 -->

<!-- どのマッパーインターフェースと対応するかを指定する -->
<mapper namespace="jp.co.ndensan.reams.db.dba.persistence.mapper.basic.DbT5051KoseiShichosonMasterMapper">

    <!--テーブルDbT1001HihokenshaDaichoの項目をDbT1001HihokenshaDaichoEntityの項目にマッピングします。-->
    <resultMap id="resultMap_getKoseiShichosonMasterList" type="jp.co.ndensan.reams.db.dbe.entity.db.basic.DbT5051KoseiShichosonMasterEntity" autoMapping="true">
    <!--  <result property="shichosonCode" column="DbT5051Kosei_shichosonCode" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.LasdecCodeTypeHandler" />
     <result property="shoKisaiHokenshaNo" column="DbT5051Kosei_shoKisaiHokenshaNo" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.ShoKisaiHokenshaNoTypeHandler" /> 
     <result property="shichosonMeisho" column="DbT5051Kosei_shichosonMeisho" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler" />-->
    </resultMap>
      <select id="getKoseiShichosonMasterList" resultMap="resultMap_getKoseiShichosonMasterList"  fetchSize="500">
	SELECT
                DbT5051Kosei."shichosonCode" as "DbT5051Kosei_shichosonCode",
                DbT5051Kosei."shoKisaiHokenshaNo" as "DbT5051Kosei_shoKisaiHokenshaNo",
                DbT5051Kosei. "shichosonMeisho" as "DbT5051Kosei_shichosonMeisho"
          FROM
                rgdb."DbT5051KoseiShichosonMaster" as DbT5051Kosei
          WHERE
                 DbT5051Kosei."gappeiKyuShichosonKubun"='0'
           AND
                 DbT5051Kosei."kanyuYMD"<![CDATA[<=]]> systemDate
           AND
                 DbT5051Kosei."ridatsuYMD" = '0'
	</select>

</mapper>
