    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>jp.co.ndensan.reams.uz.maven.plugins</groupId>
                    <artifactId>myBatis-maven-plugin</artifactId>
                    <version>1.11.0</version>
                </plugin>
                <plugin>
                    <groupId>jp.co.ndensan.reams.uz.maven.plugins</groupId>
                    <artifactId>myBatisValidator-maven-plugin</artifactId>
                    <version>1.10.0</version>
                    <dependencies>
                        <dependency>
                            <groupId>jp.co.ndensan.reams.uz</groupId>
                            <artifactId>framework</artifactId>
                            <version>1.9.0</version>
                        </dependency>
                    </dependencies>
                </plugin>
            </plugins>
        </pluginManagement>

        <plugins>
            <plugin>
                <groupId>jp.co.ndensan.reams.uz.maven.plugins</groupId>
                <artifactId>myBatis-maven-plugin</artifactId>
                <version>1.11.0</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>createMyBatisSettings</goal>
                        </goals>
                        <phase>process-sources</phase>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>jp.co.ndensan.reams.uz.maven.plugins</groupId>
                <artifactId>myBatisValidator-maven-plugin</artifactId>
                <version>1.10.0</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>createMyBatisSettings</goal>
                        </goals>
                        <phase>prepare-package</phase>
                    </execution>
                </executions>
                <configuration>
                    <checkmode>full</checkmode>
                </configuration>
            </plugin>
        </plugins>
    </build>
