            <id>kibanJenkins</id>
            <name>r5 maven repository</name>
            <url>http://zrtechwiki.ndensan.co.jp/mvn/repo/</url>
            <releases>
                <updatePolicy>always</updatePolicy>
            </releases>
            <snapshots>
