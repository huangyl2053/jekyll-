<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 概要： DbT5115Imageマスタとエンティティのマッピング用XMLです。 -->
<!-- 作者： n1070 久保裕是 -->

<!-- どのマッパーインターフェースと対応するかを指定する -->
<mapper namespace="jp.co.ndensan.reams.db.dbz.persistence.db.mapper.basic.IDbT5115ImageMapper">
    
    <!-- 概要： 削除されたデータ以外を全件取得するSQLです。 -->
    <sql id="noDeleted_DbT5115Image">
     SELECT
         <include refid="jp.co.ndensan.reams.db.dbz.persistence.db.mapper.basic.IDbT5115ImageMapper.allColumns_DbT5115Image" />
     FROM
         rgdb."DbT5115Image"
     WHERE
         rgdb."DbT5115Image"."isDeleted" = FALSE
    </sql>

    <sql id="allColumns_DbT5115Image">
         "DbT5115Image"."shinseishoKanriNo" as "dbT5115Image_shinseishoKanriNo",
         "DbT5115Image"."insertDantaiCd" as "dbT5115Image_insertDantaiCd",
         "DbT5115Image"."insertTimestamp" as "dbT5115Image_insertTimestamp",
         "DbT5115Image"."insertReamsLoginId" as "dbT5115Image_insertReamsLoginId",
         "DbT5115Image"."insertContextId" as "dbT5115Image_insertContextId",
         "DbT5115Image"."isDeleted" as "dbT5115Image_isDeleted",
         "DbT5115Image"."updateCount" as "dbT5115Image_updateCount",
         "DbT5115Image"."lastUpdateTimestamp" as "dbT5115Image_lastUpdateTimestamp",
         "DbT5115Image"."lastUpdateReamsLoginId" as "dbT5115Image_lastUpdateReamsLoginId",
         "DbT5115Image"."imageSharedFileId" as "dbT5115Image_imageSharedFileId"
    </sql>


    <!--テーブルDbT5115Imageの項目をDbT5115ImageEntityの項目にマッピングします。-->
    <resultMap id="ResultMap_DbT5115ImageEntity" type="jp.co.ndensan.reams.db.dbz.entity.db.basic.DbT5115ImageEntity" autoMapping="true">
        <id property="shinseishoKanriNo" column="dbT5115Image_shinseishoKanriNo" typeHandler="jp.co.ndensan.reams.db.dbx.entity.typehandlers.ShinseishoKanriNoTypeHandler"/>
        <result property="insertDantaiCd" column="dbT5115Image_insertDantaiCd" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <result property="insertTimestamp" column="dbT5115Image_insertTimestamp" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RDateTimeTypeHandler"/>
        <result property="insertReamsLoginId" column="dbT5115Image_insertReamsLoginId" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <result property="insertContextId" column="dbT5115Image_insertContextId" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.UUIDTypeHandler"/>
        <result property="isDeleted" column="dbT5115Image_isDeleted" />
        <result property="updateCount" column="dbT5115Image_updateCount" />
        <result property="lastUpdateTimestamp" column="dbT5115Image_lastUpdateTimestamp" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RDateTimeTypeHandler"/>
        <result property="lastUpdateReamsLoginId" column="dbT5115Image_lastUpdateReamsLoginId" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <result property="imageSharedFileId" column="dbT5115Image_imageSharedFileId" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RDateTimeTypeHandler"/>
    </resultMap>

    <select id="getEntity" resultMap="ResultMap_DbT5115ImageEntity">
        <!--SELECT
        <include refid="allColumns_DbT5115Image"/>
        FROM
        rgdb."DbT5115Image"-->
    </select>
</mapper>
