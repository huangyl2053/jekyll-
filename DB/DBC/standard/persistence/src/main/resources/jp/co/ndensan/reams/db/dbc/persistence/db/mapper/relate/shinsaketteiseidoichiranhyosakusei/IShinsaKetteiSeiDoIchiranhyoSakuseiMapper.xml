<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 概要： 支払方法情報のマッピング用XMLです。 -->
<!-- @reamsid_L @reamsid_L DBC-2500-030 jiangxiaolong -->

<!-- どのマッパーインターフェースと対応するかを指定する -->
<mapper namespace="jp.co.ndensan.reams.db.dbc.persistence.db.mapper.relate.shinsaketteiseidoichiranhyosakusei.IShinsaKetteiSeiDoIchiranhyoSakuseiMapper">

    <select id="select帳票出力対象データ" 
            resultMap="resultMap_KyufuhiShinsaKetteiSeikyuMeisaihyoEntity" parameterType="jp.co.ndensan.reams.db.dbc.definition.mybatisprm.kokuhorenkyoutsuu.KokuhorenIchiranhyoMybatisParameter" fetchSize="500" resultOrdered="true">
        SELECT *
        FROM 
        "DbWT1611SinsaKetteiSeikyuMeisai"
        INNER JOIN 
        "DbWT1612SinsaKetteiSeikyuKogaku"
        ON "DbWT1611SinsaKetteiSeikyuMeisai"."meisaiRirekiNo" = "DbWT1612SinsaKetteiSeikyuKogaku"."kogakuRirekiNo"
        INNER JOIN
        "DbWT1613SinsaKetteiSeikyuGokei"
        ON "DbWT1611SinsaKetteiSeikyuMeisai"."meisaiRirekiNo" = "DbWT1613SinsaKetteiSeikyuGokei"."gokeiRirekiNo"
        ${出力順}
    </select>
    <resultMap id="resultMap_KyufuhiShinsaKetteiSeikyuMeisaihyoEntity" type="jp.co.ndensan.reams.db.dbc.business.report.kyufuhishinsaketteiseikyumeisaihyo.KyufuhiShinsaKetteiSeikyuMeisaihyoEntity" autoMapping="true">
        <association property="明細テータ" resultMap="resultMap_DbWT1611SinsaKetteiSeikyuMeisai"/>
        <association property="高額テータ" resultMap="resultMap_DbWT1612SinsaKetteiSeikyuKogaku"/>
        <association property="合計テータ" resultMap="resultMap_DbWT1613SinsaKetteiSeikyuGokei"/>
    </resultMap>
    
    <resultMap id="resultMap_DbWT1611SinsaKetteiSeikyuMeisai" type="jp.co.ndensan.reams.db.dbc.business.report.kyufuhishinsaketteiseikyumeisaihyo.DbWT1611SinsaKetteiSeikyuMeisai" autoMapping="true">
        <id property="履歴番号" column="meisaiRirekiNo"/>
        <result property="事業所番号" column="jigyoshoNo" typeHandler="jp.co.ndensan.reams.db.dbx.entity.typehandlers.JigyoshaNoTypeHandler"/>
        <result property="事業所名" column="jigyoshoName" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <result property="サービス提供年月" column="serviceTeikyouYM" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.FlexibleYearMonthTypeHandler"/>
        <result property="サービス種類コード" column="serviceShuruiCode" typeHandler="jp.co.ndensan.reams.db.dbx.entity.typehandlers.ServiceShuruiCodeTypeHandler"/>
        <result property="サービス種類名" column="serviceShuruiMei" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <result property="件数" column="kensu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
        <result property="日数_回数" column="nissuKaisu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
        <result property="単位数" column="tanisu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
        <result property="金額" column="kingaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
        <result property="介護給付費_総合事業費" column="kaigoKyufuhiSogoJigyohi" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
        <result property="利用者負担額" column="riyoshaFutangaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
        <result property="公費負担額" column="kohiFutangaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
    </resultMap> 
    
    <resultMap id="resultMap_DbWT1612SinsaKetteiSeikyuKogaku" type="jp.co.ndensan.reams.db.dbc.business.report.kyufuhishinsaketteiseikyumeisaihyo.DbWT1612SinsaKetteiSeikyuKogaku" autoMapping="true">
         <id property="履歴番号" column="kogakuRirekiNo"/>
        <result property="高額_高額該当件数" column="kogakuGaitoKensu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
        <result property="高額_高額介護サービス費" column="kogakuKaigoServicehi" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
        <result property="高額_公費負担額" column="kogakuKohiFutangaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
    </resultMap> 
    
    <resultMap id="resultMap_DbWT1613SinsaKetteiSeikyuGokei" type="jp.co.ndensan.reams.db.dbc.business.report.kyufuhishinsaketteiseikyumeisaihyo.DbWT1613SinsaKetteiSeikyuGokei" autoMapping="true">
        <id property="履歴番号" column="gokeiRirekiNo"/>
        <result property="保険者番号" column="hokenshaNo" typeHandler="jp.co.ndensan.reams.db.dbx.entity.typehandlers.HokenshaNoTypeHandler"/>
        <result property="保険者名" column="hokenshaName" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <result property="審査年月" column="shinsaYM" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.FlexibleYearMonthTypeHandler"/>
        <result property="証記載保険者番号" column="shoKisaiHokenshaNo" typeHandler="jp.co.ndensan.reams.db.dbx.entity.typehandlers.ShoKisaiHokenshaNoTypeHandler"/>
        <result property="国保連合会名" column="kokuhorenName" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <result property="合計_件数" column="gokeiKensu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
        <result property="合計_単位数" column="gokeiTanisu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
        <result property="合計_金額" column="gokeiKingaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
        <result property="合計_介護給付費_総合事業費" column="gokeiKaigoKyufuhiSogoJigyohi" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
        <result property="合計_利用者負担額" column="gokeiRiyoshaFutangaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
        <result property="合計_公費負担額" column="gokeiKohiFutangaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
    </resultMap> 
</mapper>