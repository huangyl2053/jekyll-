<!-- 概要： 徴収方法変更のマッピング用XMLです。 -->
<!-- @reamsid_L DBC-4770-010 zhengshenlei -->
<!-- どのマッパーインターフェースと対応するかを指定する -->
<mapper namespace="jp.co.ndensan.reams.db.dbc.persistence.db.mapper.relate.koga.IKogakuShikyuShinseishoIkkatsuHakkoMapper">
    <select id="getServiceTeikyoByDbT3056" parameterType="jp.co.ndensan.reams.db.dbc.definition.mybatisprm.koga.KogakuShikyuShinseishoIkkatsuHakkoMapperParameter" 
     resultMap="ResultMap_DbT3056KogakuShikyuShinseiEntity"  fetchSize="500"  resultOrdered="true">
         SELECT 
            T1.*
         FROM(
            SELECT T.*
            FROM(
            SELECT 
                KOGAKUSHIKYU.*,
                row_number() OVER (PARTITION BY KOGAKUSHIKYU."hihokenshaNo" ORDER BY KOGAKUSHIKYU."serviceTeikyoYM" DESC, KOGAKUSHIKYU."shoKisaiHokenshaNo" DESC,
                KOGAKUSHIKYU."rirekiNo" DESC) AS rrkno
            FROM rgdb."DbT3056KogakuShikyuShinsei" AS KOGAKUSHIKYU
            ) AS T
<!--            WHERE
                T.rrkno = 1-->
        ) AS T1
        WHERE
            T1."hihokenshaNo" = #{hihokenshaNo}
        AND T1."shoKisaiHokenshaNo" = #{shoKisaiHokenshaNo}
    </select>
    
    <select id="getServiceTeikyoByDbT3110" parameterType="jp.co.ndensan.reams.db.dbc.definition.mybatisprm.koga.KogakuShikyuShinseishoIkkatsuHakkoMapperParameter" 
     resultMap="ResultMap_DbT3110JigyoKogakuShikyuShinseiEntity"  fetchSize="500"  resultOrdered="true">
         SELECT 
            T1.*
         FROM(
            SELECT T.*
            FROM(
            SELECT 
                SHOKANHANTEI.*,
                row_number() OVER (PARTITION BY SHOKANHANTEI."hihokenshaNo" ORDER BY SHOKANHANTEI."serviceTeikyoYM" DESC, SHOKANHANTEI."shoKisaiHokenshaNo" DESC,
                SHOKANHANTEI."rirekiNo" DESC) AS rrkno
            FROM rgdb."DbT3110JigyoKogakuShikyuShinsei" AS SHOKANHANTEI
            ) AS T
<!--            WHERE
                T.rrkno = 1-->
        ) AS T1
        WHERE
            T1."hihokenshaNo" = #{hihokenshaNo}
        AND T1."shoKisaiHokenshaNo" = #{shoKisaiHokenshaNo}
    </select>
    
