            <result property="事業者番号" column="dbWT3064_jigyoshoNo" typeHandler="jp.co.ndensan.reams.db.dbx.entity.typehandlers.JigyoshaNoTypeHandler"/>
            <result property="事業者名" column="dbWT3064_jigyoshoName" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
            <result property="被保険者番号" column="dbWT0001_orgHihokenshaNo" typeHandler="jp.co.ndensan.reams.db.dbx.entity.typehandlers.HihokenshaNoTypeHandler"/>
            <result property="被保険者氏名" column="dbWT0001_orgHihokenshaShimei" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
            <result property="サービス提供年月" column="dbWT3064_serviceTeikyoYM" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.FlexibleYearMonthTypeHandler"/>
            <result property="サービス種類コード" column="dbWT3064_serviceShuruiCode" typeHandler="jp.co.ndensan.reams.db.dbx.entity.typehandlers.ServiceShuruiCodeTypeHandler"/>
            <result property="サービス種類名" column="dbWT3064_serviceShuruiMei" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
            <result property="申立事由コード" column="dbWT3064_kagomoushitateJiyuCode" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.CodeTypeHandler"/>
            <result property="申立事由" column="dbWT3064_kagomoushitateJiyu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
            <result property="当初請求単位数" column="dbWT3064_toshoSeikyuTanisu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="原審単位数" column="dbWT3064_genshinSeikyuTanisu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="決定単位数" column="dbWT3064_ketteiTanisu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="調整単位数" column="dbWT3064_choseiTanisu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="公費負担額" column="dbWT3064_hokenshaFutangaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="総合事業費_決定_件数" column="dbWT3063_kaigoKyufuhiKetteiKensu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="総合事業費_決定_単位数" column="dbWT3063_kaigoKyufuhiKetteiTanisu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="総合事業費_決定_負担額" column="dbWT3063_kaigoKyufuhiKetteiFutangaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="総合事業費_調整_件数" column="dbWT3063_kaigoKyufuhiChoseiKensu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="総合事業費_調整_単位数" column="dbWT3063_kaigoKyufuhiChoseiTanisu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="総合事業費_調整_負担額" column="dbWT3063_kaigoKyufuhiChoseiFutangaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="識別コード" column="dbWT0001_shikibetsuCode" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.ShikibetsuCodeTypeHandler"/>
            <result property="連番" column="row_number" />
    </resultMap>
    
    <sql id="allColumns_SogojigyohiSaishinsaKetteiTsuchishoChohyo">
        DbWT3063."kokuhorenName" AS dbWT3063_kokuhorenName,
        DbWT3063."shinsakaiName" AS dbWT3063_shinsakaiName,
        DbWT3063."shoKisaiHokenshaNo" AS dbWT3063_shoKisaiHokenshaNo,
        DbWT3063."shoKisaiHokenshaName" AS dbWT3063_shoKisaiHokenshaName,
        DbWT3064."toriatsukaiYM" AS dbWT3064_toriatsukaiYM,
        DbWT3064."jigyoshoNo" AS dbWT3064_jigyoshoNo,
        DbWT3064."jigyoshoName" As dbWT3064_jigyoshoName,
        DbWT0001."orgHihokenshaNo" As dbWT0001_orgHihokenshaNo,
        DbWT0001."orgHihokenshaShimei" As  dbWT0001_orgHihokenshaShimei,
