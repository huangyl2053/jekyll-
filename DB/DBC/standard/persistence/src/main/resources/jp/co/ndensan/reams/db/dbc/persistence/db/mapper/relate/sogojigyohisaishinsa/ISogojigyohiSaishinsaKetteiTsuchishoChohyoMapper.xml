        DbWT3064."serviceShuruiCode" AS dbWT3064_serviceShuruiCode,
        DbWT3064."serviceShuruiMei" AS dbWT3064_serviceShuruiMei,
        DbWT3064."kagomoushitateJiyuCode" AS dbWT3064_kagomoushitateJiyuCode,
        DbWT3064."kagomoushitateJiyu" AS dbWT3064_kagomoushitateJiyu,
        DbWT3064."toshoSeikyuTanisu" AS dbWT3064_toshoSeikyuTanisu,
        DbWT3064."genshinSeikyuTanisu" AS dbWT3064_genshinSeikyuTanisu,
        DbWT3064."ketteiTanisu" AS dbWT3064_ketteiTanisu,
        DbWT3064."choseiTanisu" AS dbWT3064_choseiTanisu,
        DbWT3064."hokenshaFutangaku" AS dbWT3064_hokenshaFutangaku,
        DbWT3063."kaigoKyufuhiKetteiKensu" AS dbWT3063_kaigoKyufuhiKetteiKensu,
        DbWT3063."kaigoKyufuhiKetteiTanisu" AS dbWT3063_kaigoKyufuhiKetteiTanisu,
        DbWT3063."kaigoKyufuhiKetteiFutangaku" AS dbWT3063_kaigoKyufuhiKetteiFutangaku,
        DbWT3063."kaigoKyufuhiChoseiKensu" AS dbWT3063_kaigoKyufuhiChoseiKensu,
        DbWT3063."kaigoKyufuhiChoseiTanisu" AS dbWT3063_kaigoKyufuhiChoseiTanisu,
        DbWT3063."kaigoKyufuhiChoseiFutangaku" AS dbWT3063_kaigoKyufuhiChoseiFutangaku,
        DbWT0001."shikibetsuCode" As  dbWT0001_shikibetsuCode,
        ROW_NUMBER() OVER() AS row_number
    </sql>
        
</mapper>        
        
        
        
        