                   autoMapping="true">
            <id column="row_number"/>
            <result property="国保連合会名" column="dbWT3063_kokuhorenName" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
