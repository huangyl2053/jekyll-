<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- @reamsid_L DBC-0980-420 zhangrui -->
<mapper namespace="jp.co.ndensan.reams.db.dbc.persistence.db.mapper.relate.kagoketteihokenshainkouhifutann.IKohifutanshaChohyoMapper">
    <select id="get帳票出力対象データ"
                resultMap="resultMap_KagoKetteiKohifutanshaChohyoEntity" parameterType="java.util.Map"  fetchSize="500" resultOrdered="true">
        SELECT 
        <include refid="allColumns_kohifutanshaChohyo" />
        FROM "DbWT3060KagoKetteiShukei" AS DbWT3060
        INNER JOIN "DbWT3061KagoKetteiMeisai" AS DbWT3061 ON DbWT3060."hdrRirekiNo" = DbWT3061."rirekiNo"
        INNER JOIN "DbWT0001Hihokensha" AS DbWT0001 ON DbWT3061."renban" = DbWT0001."meisaiRenban"
        ${出力順}
    </select>

    <resultMap id="resultMap_KagoKetteiKohifutanshaChohyoEntity"
                   type="jp.co.ndensan.reams.db.dbc.entity.db.relate.kagoketteikohifutanshain.KagoKetteiKohifutanshaChohyoEntity"
                   autoMapping="true">
            <result property="公費負担者番号" column="dbWT3060_ShoHokenshaNo" typeHandler="jp.co.ndensan.reams.db.dbx.entity.typehandlers.ShoKisaiHokenshaNoTypeHandler"/>
            <result property="公費負担者名" column="dbWT3060_ShoHokenshaMei" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
            <result property="no" column="dbWT3061_renban" />
            <result property="取扱年月" column="dbWT3061_toriatsukaiYm" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.FlexibleYearMonthTypeHandler"/>
            <result property="事業者番号" column="dbWT3061_jigyoshoNo" typeHandler="jp.co.ndensan.reams.db.dbx.entity.typehandlers.JigyoshaNoTypeHandler"/>
            <result property="公費受給者番号" column="dbWT3061_kohiJukyushaNo" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
            <result property="公費受給者名" column="dbWT0001_orgHihokenshaShimei" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
            <result property="サービ提供年月" column="dbWT3061_serviceTeikyoYm" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.FlexibleYearMonthTypeHandler"/>
            <result property="サービス種類コード" column="dbWT3061_serviceShuruiCode" typeHandler="jp.co.ndensan.reams.db.dbx.entity.typehandlers.ServiceShuruiCodeTypeHandler"/>
            <result property="過誤申立事由コード" column="dbWT3061_kagomoushitateJiyuCode" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.CodeTypeHandler"/>
            <result property="単位数" column="dbWT3061_tanisu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="事業者名" column="dbWT3061_jigyoshoMei" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
            <result property="被保険者番号" column="dbWT0001_hihokenshaNo" typeHandler="jp.co.ndensan.reams.db.dbx.entity.typehandlers.HihokenshaNoTypeHandler"/>
            <result property="サービス種類名" column="dbWT3061_serviceShuruiName" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
            <result property="過誤申立事由" column="dbWT3061_kagomoushitateJiyu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
            <result property="公費負担額" column="dbWT3061_hokenshaFutangaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="介護給付費の件数" column="dbWT3060_kaigoKyufuhiKensu" />
            <result property="介護給付費の単位数" column="dbWT3060_kaigoKyufuhiTanisu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="介護給付費の負担額" column="dbWT3060_kaigoKyufuhiFutangaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="高額介護サービス費の件数" column="dbWT3060_kogakuServicehiKensu" />
            <result property="高額介護サービス費の単位数" column="dbWT3060_kogakuServicehiTanisu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="高額介護サービス費の負担額" column="dbWT3060_kogakuServicehiFutangaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="特定入所者介護費等の件数" column="dbWT3060_tokuteiNyushoshaServicehiKensu" />
            <result property="特定入所者介護費等の単位数" column="dbWT3060_tokuteiNyushoshaServicehiTanisu" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="特定入所者介護費等の負担額" column="dbWT3060_tokuteiNyushoshaServicehiFutangaku" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.DecimalTypeHandler"/>
            <result property="識別コード" column="dbWT0001_shikibetsuCode" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.ShikibetsuCodeTypeHandler"/>
            <result property="公費証記載保険者番号" column="dbWT3061_kohiShokisaiHokenshaNo" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
    </resultMap>

    <sql id="allColumns_kohifutanshaChohyo">
        DbWT3060."shoKisaiHokenshaNo" AS dbWT3060_ShoHokenshaNo,
        DbWT3060."shoKisaiHokenshaName" AS dbWT3060_ShoHokenshaMei,
        DbWT3061."renban" AS dbWT3061_renban,
        DbWT3061."toriatsukaiYM" AS dbWT3061_toriatsukaiYm,
        DbWT3061."jigyoshoNo" AS dbWT3061_jigyoshoNo,
        DbWT3061."kohiJukyushaNo" AS dbWT3061_kohiJukyushaNo,
        DbWT0001."meisho" AS dbWT0001_orgHihokenshaShimei,
        DbWT3061."serviceTeikyoYM" AS dbWT3061_serviceTeikyoYm,
        DbWT3061."serviceShuruiCode" AS dbWT3061_serviceShuruiCode,
        DbWT3061."kagomoushitateJiyuCode" AS dbWT3061_kagomoushitateJiyuCode,
        DbWT3061."tanisu" AS dbWT3061_tanisu,
        DbWT3061."jigyoshoName" AS dbWT3061_jigyoshoMei,
        DbWT0001."hihokenshaNo" AS dbWT0001_hihokenshaNo,
        DbWT3061."serviceShuruiName" AS dbWT3061_serviceShuruiName,
        DbWT3061."kagomoushitateJiyu" AS dbWT3061_kagomoushitateJiyu,
        DbWT3061."hokenshaFutangaku" AS dbWT3061_hokenshaFutangaku,
        DbWT3060."kaigoKyufuhiKensu" AS dbWT3060_kaigoKyufuhiKensu,
        DbWT3060."kaigoKyufuhiTanisu" AS dbWT3060_kaigoKyufuhiTanisu,
        DbWT3060."kaigoKyufuhiFutangaku" AS dbWT3060_kaigoKyufuhiFutangaku,
        DbWT3060."kogakuServicehiKensu" AS dbWT3060_kogakuServicehiKensu,
        DbWT3060."kogakuServicehiTanisu" AS dbWT3060_kogakuServicehiTanisu,
        DbWT3060."kogakuServicehiFutangaku" AS dbWT3060_kogakuServicehiFutangaku,
        DbWT3060."tokuteiNyushoshaServicehiKensu" AS dbWT3060_tokuteiNyushoshaServicehiKensu,
        DbWT3060."tokuteiNyushoshaServicehiTanisu" AS dbWT3060_tokuteiNyushoshaServicehiTanisu,
        DbWT3060."tokuteiNyushoshaServicehiFutangaku" AS dbWT3060_tokuteiNyushoshaServicehiFutangaku,
        DbWT0001."shikibetsuCode" AS dbWT0001_shikibetsuCode,
        DbWT3061."kohiShokisaiHokenshaNo" AS dbWT3061_kohiShokisaiHokenshaNo
    </sql>

</mapper>
