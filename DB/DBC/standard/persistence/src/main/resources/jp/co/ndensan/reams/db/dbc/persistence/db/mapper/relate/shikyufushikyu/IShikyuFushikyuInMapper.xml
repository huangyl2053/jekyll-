<mapper namespace="jp.co.ndensan.reams.db.dbc.persistence.db.mapper.relate.shikyufushikyu.IShikyuFushikyuInMapper">
    
    <select id="get通知書および一覧情報" resultMap="resultMap_ShikyuFushikyuTsuchishoIchiranEntity" fetchSize="500" resultOrdered="true">
        select <include refid="allColumns_DbWT38B1KogakuGassanShikyuFushikyuKettei" />,
            <include refid="allColumns_DbWT38G1KetteishaIchiran" />
        from "DbWT38B1KogakuGassanShikyuFushikyuKettei" AS DbWT38B1 
        left join 
        "DbWT38G1KetteishaIchiran" AS DbWT38G1
        on
        DbWT38B1."chohyoKanrenzukeNo" = DbWT38G1."no"
    </select>
    
    <select id="get一覧データのみ一覧情報" resultMap="resultMap_DbWT38G1KetteishaIchiranTempEntity" fetchSize="500" resultOrdered="true">
        select <include refid="allColumns_DbWT38G1KetteishaIchiran" />
        from "DbWT38G1KetteishaIchiran" AS DbWT38G1
        WHERE NOT EXISTS (
            SELECT 'X' FROM "DbWT38B1KogakuGassanShikyuFushikyuKettei" AS DbWT38B1
            WHERE DbWT38B1."chohyoKanrenzukeNo" = DbWT38G1."no"
        )
    </select>
    
    <select id="get削除予定前回処理データ" resultMap="jp.co.ndensan.reams.db.dbc.persistence.db.mapper.basic.IDbT3074KogakuGassanShikyuFushikyuKetteiMapper.ResultMap_DbT3074KogakuGassanShikyuFushikyuKetteiEntity" 
        parameterType="jp.co.ndensan.reams.db.dbc.definition.mybatisprm.dbc120140.ShikyuFushikyuMyBatisParameter"  fetchSize="500" resultOrdered="true">
        SELECT
         <include refid="jp.co.ndensan.reams.db.dbc.persistence.db.mapper.basic.IDbT3074KogakuGassanShikyuFushikyuKetteiMapper.allColumns_DbT3074KogakuGassanShikyuFushikyuKettei" />
     FROM
         rgdb."DbT3074KogakuGassanShikyuFushikyuKettei"
     WHERE
         rgdb."DbT3074KogakuGassanShikyuFushikyuKettei"."isDeleted" = FALSE
         AND rgdb."DbT3074KogakuGassanShikyuFushikyuKettei"."uketoriYM" = #{処理年月}
         <if test="処理区分 == 1 || 処理区分 == 2">
            AND EXISTS (
             SELECT 'X' FROM rgdb."DbT3070KogakuGassanJikoFutanGaku" 
             WHERE "DbT3070KogakuGassanJikoFutanGaku"."hihokenshaNo" = "DbT3074KogakuGassanShikyuFushikyuKettei"."hihokenshaNo" 
             AND "DbT3070KogakuGassanJikoFutanGaku"."taishoNendo" = "DbT3074KogakuGassanShikyuFushikyuKettei"."taishoNendo"
             AND "DbT3070KogakuGassanJikoFutanGaku"."hokenshaNo" = "DbT3074KogakuGassanShikyuFushikyuKettei"."hokenshaNo"
             AND "DbT3070KogakuGassanJikoFutanGaku"."shikyuShinseishoSeiriNo" = "DbT3074KogakuGassanShikyuFushikyuKettei"."shikyuSeiriNo"
             AND "DbT3070KogakuGassanJikoFutanGaku"."rirekiNo" = "DbT3074KogakuGassanShikyuFushikyuKettei"."rirekiNo"
             AND "DbT3070KogakuGassanJikoFutanGaku"."koki_KokuhoShoriKubun" = #{処理区分}
             )
        </if>
    </select>
    
    <select id="get口座登録" resultMap="resultMap_ShikyuFushikyuKetteiKozaEntity" 
            parameterType="jp.co.ndensan.reams.db.dbc.definition.mybatisprm.dbc120140.ShikyuFushikyuKozaMyBatisParameter" fetchSize="500" resultOrdered="true">
        SELECT * FROM 
        (select <include refid="allColumns_DbWT38B1KogakuGassanShikyuFushikyuKettei" />,
        <include refid="jp.co.ndensan.reams.db.dbc.persistence.db.mapper.relate.kokuhorenkyoutsuu.IKokuhorenKyoutsuuTempTableMapper.allColumns_DbWT0001Hihokensha" />
        FROM "DbWT38B1KogakuGassanShikyuFushikyuKettei" AS DbWT38B1 
        INNER JOIN 
        "DbWT0001Hihokensha" AS TEMP 
        ON DbWT38B1."renban" = TEMP."meisaiRenban"
        WHERE DbWT38B1."shikyuKubunCode" = '1' AND DbWT38B1."shiharaiHohoKubun" = '2') AS 高額合算
        <if test="hasValidKamoku">
        LEFT JOIN (<include refid="jp.co.ndensan.reams.ua.uax.persistence.db.mapper.IKozaRelateMapper.tokuteiKozaSearch"/>) AS 口座取得SQL
        ON  高額合算.dbWT0001HihokenshaTemp_shikibetsuCode = 口座取得SQL."uaT0310Koza_shikibetsuCode"
        AND 高額合算.DbWT38B1_kinyuKikanCode = 口座取得SQL."uaT0310Koza_kinyuKikanCode"
        AND 高額合算.DbWT38B1_kinyuKikanShitenCode = 口座取得SQL."uaT0310Koza_kinyuKikanShitenCode"
        AND 高額合算.DbWT38B1_kozaShumoku = 口座取得SQL."uaT0310Koza_yokinShubetsu"
        AND 高額合算.DbWT38B1_kozaNo = 口座取得SQL."uaT0310Koza_kozaNo"
        </if>
    </select>
    
    <select id="get業務別主キー" resultType="jp.co.ndensan.reams.uz.uza.lang.RString" 
            parameterType="jp.co.ndensan.reams.db.dbc.definition.mybatisprm.dbc120140.ShikyuFushikyuMyBatisParameter" fetchSize="1" resultOrdered="true">
        SELECT "gyomubetsuPrimaryKey" 
        FROM rgua."UaT0100GyomubetsuPrimaryKey"
        WHERE "gyomuCode" = 'DB'
        AND "subGyomuCode" = 'DBC'
        AND "kamokuCode" = #{科目コード}
    </select>
    
    <select id="get最大履歴番号" resultMap="resultMap_ShikyuFushikyuRirikoNoEntity" fetchSize="500" resultOrdered="true">
        SELECT
            <include refid="allColumns_DbWT38B1KogakuGassanShikyuFushikyuKettei" />,
            <include refid="jp.co.ndensan.reams.db.dbc.persistence.db.mapper.relate.kokuhorenkyoutsuu.IKokuhorenKyoutsuuTempTableMapper.allColumns_DbWT0001Hihokensha" />
            ,max_rrkno
        FROM
            "DbWT38B1KogakuGassanShikyuFushikyuKettei" AS DbWT38B1
                INNER JOIN "DbWT0001Hihokensha" AS TEMP
                    ON DbWT38B1. "renban" = TEMP. "meisaiRenban"
                INNER JOIN (
                        SELECT
                            "hihokenshaNo"
                            ,"taishoNendo"
                            ,"hokenshaNo"
                            ,"shikyuSeiriNo"
                            ,MAX ("rirekiNo") AS max_rrkno
                        FROM
                            rgdb. "DbT3074KogakuGassanShikyuFushikyuKettei" AS DbT3074
                        GROUP BY
                            DbT3074. "hihokenshaNo"
                            ,DbT3074. "taishoNendo"
                            ,DbT3074. "hokenshaNo"
                            ,DbT3074. "shikyuSeiriNo"
                    ) AS 最大履歴
                    ON DbWT38B1. "hihokenshaNoIn" = 最大履歴. "hihokenshaNo"
            AND DbWT38B1. "taishoNendo" = 最大履歴. "taishoNendo"
            AND DbWT38B1. "shoKisaiHokenshaNo" = 最大履歴. "hokenshaNo"
            AND DbWT38B1. "shikyuShinseishoSeiriNo" = 最大履歴. "shikyuSeiriNo"
    </select>
    
    <select id="get高額合算支給不支給決定一時データ全件" resultMap="resultMap_DbWT38B1KogakuGassanShikyuFushikyuKetteiTempEntity" fetchSize="500" resultOrdered="true">
        SELECT <include refid="allColumns_DbWT38B1KogakuGassanShikyuFushikyuKettei" />
        FROM "DbWT38B1KogakuGassanShikyuFushikyuKettei" AS DbWT38B1
    </select>
    
    <select id="get帳票出力対象データ" resultMap="resultMap_ShikyuFushikyuChohyoEntity" fetchSize="500" resultOrdered="true">
        SELECT <include refid="allColumns_DbWT38B1KogakuGassanShikyuFushikyuKettei" />,
        <include refid="jp.co.ndensan.reams.db.dbc.persistence.db.mapper.relate.kokuhorenkyoutsuu.IKokuhorenKyoutsuuTempTableMapper.allColumns_DbWT0001Hihokensha" />
        FROM "DbWT38B1KogakuGassanShikyuFushikyuKettei" AS DbWT38B1 
        INNER JOIN 
        "DbWT0001Hihokensha" AS TEMP 
        ON DbWT38B1."renban" = TEMP."meisaiRenban"
        ${出力順}
    </select>
    
