<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 概要： 画面設計_DBC4210011_種類支給限度額登録のマッピング用XMLです。 -->
<!-- @reamsid_L DBC-3440-010 lihang -->

<mapper namespace="jp.co.ndensan.reams.db.dbc.persistence.db.mapper.relate.shuruishikyugendogakumain.IShuruiShikyuGendogakuMainMapper" >
    
    <delete id="deletEntity" 
            parameterType="jp.co.ndensan.reams.db.dbc.definition.mybatisprm.shuruishikyugendogakumain.ShuruiShikyuGendogakuMainListParameter">
        DELETE
        FROM
        rgdb."DbT7111ServiceShuruiShikyuGendoGaku" AS A
        WHERE
        A."tekiyoKaishiYM" =#{適用開始年月} AND
        A."rirekiNo" =#{履歴番号} AND
        A."serviceShuruiCode" =#{サービス種類コード} AND
        A."yoKaigoJotaiKubun" =#{要介護状態区分}
</delete>
</mapper>
