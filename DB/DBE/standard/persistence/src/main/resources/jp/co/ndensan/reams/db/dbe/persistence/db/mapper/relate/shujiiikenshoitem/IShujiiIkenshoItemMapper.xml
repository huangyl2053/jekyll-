    </resultMap>

    <select id="select主治医意見書" resultMap="ResultMap_ShujiiIkenshoIkenItemEntity" fetchSize="500" resultOrdered="true">
        SELECT
        "DbT5304ShujiiIkenshoIkenItem"."shinseishoKanriNo" as "dbT5304ShujiiIkenshoIkenItem_shinseishoKanriNo",
        "DbT5304ShujiiIkenshoIkenItem"."ikenshoIraiRirekiNo" as "dbT5304ShujiiIkenshoIkenItem_ikenshoIraiRirekiNo",
        "DbT5304ShujiiIkenshoIkenItem"."remban" as "dbT5304ShujiiIkenshoIkenItem_remban",
        "DbT5304ShujiiIkenshoIkenItem"."koroshoIfShikibetsuCode" as "dbT5304ShujiiIkenshoIkenItem_koroshoIfShikibetsuCode",
        "DbT5304ShujiiIkenshoIkenItem"."ikenItem" as "dbT5304ShujiiIkenshoIkenItem_ikenItem"
        FROM
        rgdb."DbT5304ShujiiIkenshoIkenItem" as "DbT5304ShujiiIkenshoIkenItem"
        WHERE "DbT5304ShujiiIkenshoIkenItem"."shinseishoKanriNo" = #{申請書管理番号}
        AND "DbT5304ShujiiIkenshoIkenItem"."ikenshoIraiRirekiNo" =  #{依頼履歴番号}
</select>

