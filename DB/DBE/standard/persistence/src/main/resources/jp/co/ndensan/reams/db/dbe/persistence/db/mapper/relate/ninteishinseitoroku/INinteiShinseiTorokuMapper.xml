        LEFT JOIN rgdb."DbT5502ShinsakaiWariateJoho" T5502
          ON T5502."shinseishoKanriNo" = T5101."shinseishoKanriNo"
          AND T5502."shinsakaiKaisaiNo" = T5102."shinsakaiKaisaiNo"
