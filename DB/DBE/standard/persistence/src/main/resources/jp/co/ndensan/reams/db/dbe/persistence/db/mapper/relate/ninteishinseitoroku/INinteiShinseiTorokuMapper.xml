<mapper namespace="jp.co.ndensan.reams.db.dbe.persistence.db.mapper.relate.ninteishinseitoroku.INinteiShinseiTorokuMapper">
     <select id="getDataForCom" resultMap="resultMap_GetDataForCom"
             parameterType="jp.co.ndensan.reams.uz.uza.lang.RString" 
             fetchSize="500" resultOrdered="true">
            SELECT
              T5913."ninteiChosainCode"
              , T5913."chosainShimei"
              , T5910."ninteichosaItakusakiCode"
              , T5910."jigyoshaMeisho"
              , T5910."yubinNo" AS "ninteiYubinNo"
              , T5910."jusho" AS "ninteiJusho"
              , T5910."telNo" AS "ninteiTelNo"
              , T5201."ninteichosaIraiYMD"
              , T5123."ninteichosaYoteiYMD"
              , T5202."ninteichosaJisshiYMD"
              , T5910."chosaItakuKubun"
              , T5101."homonChosasakiName"
              , T5101."homonChosasakiYubinNo"
              , T5101."homonChosasakiJusho"
              , T5101."homonChosasakiTelNo"
              , T5912."shujiiCode"
              , T5912."shujiiName"
              , T5911."shujiiIryokikanCode"
              , T5911."iryoKikanMeisho"
              , T5911."yubinNo" AS "shujiiYubinNo"
              , T5911."jusho" AS "shujiiJusho"
              , T5911."telNo" AS "shujiiTelNo"
              , T5301."ikenshoSakuseiIraiYMD"
              , T5123."ikenshoTorokuYoteiYMD"
              , T5302."ikenshoKinyuYMD"
              , T5912."shiteiiFlag"
              , T5116."ichijiHanteiKekkaCode"
              , T5116."ichijiHanteiYMD"
              , T5116."ichijiHanteiKekkaNinchishoKasanCode"
              , T5102."shinsakaiShiryoSakuseiYMD"
              , T5123."ninteiShinsakaiYoteiYMD"
              , T5102."nijiHanteiYMD"
              , T5102."nijiHanteiYokaigoJotaiKubunCode"
              , T5102."nijiHanteiNinteiYukoKikan"
              , T5102."nijiHanteiNinteiYukoKaishiYMD"
              , T5102."nijiHanteiNinteiYukoShuryoYMD"
              , T5101."enkiTsuchiHakkoYMD"
              , T5101."enkiTsuchiHakkoKaisu"
              , T5101."shinseishoKanriNo"
              , T5101."koroshoIfShikibetsuCode"
              , T7051."shichosonCode"
            FROM
                rgdb."DbT5101NinteiShinseiJoho" T5101
            INNER JOIN rgdb."DbT5102NinteiKekkaJoho" T5102 
                ON T5101."shinseishoKanriNo" = T5102."shinseishoKanriNo"
                AND T5102."nijiHanteiYMD" <![CDATA[<>]]> ''
            INNER JOIN rgdb."DbT7051KoseiShichosonMaster" T7051 
                ON T5101."shoKisaiHokenshaNo" = T7051."shoKisaiHokenshaNo" 
            LEFT JOIN rgdb."DbT5201NinteichosaIraiJoho" AS T5201 
                ON T5101."shinseishoKanriNo" = T5201."shinseishoKanriNo" 
                AND T5201."logicalDeletedFlag" = False 
                AND NOT EXISTS ( 
                  SELECT
                    'X' 
                  FROM
                    rgdb."DbT5201NinteichosaIraiJoho" AS B1 
                  WHERE
                    T5201."shinseishoKanriNo" = B1."shinseishoKanriNo" 
                    AND B1."logicalDeletedFlag" = False 
                    AND T5201."ninteichosaIraiRirekiNo" <![CDATA[<]]> B1."ninteichosaIraiRirekiNo"
                ) 
              LEFT JOIN rgdb."DbT5202NinteichosahyoGaikyoChosa" AS T5202 
                ON T5101."shinseishoKanriNo" = T5202."shinseishoKanriNo" 
                AND NOT EXISTS ( 
                  SELECT
                    'X' 
                  FROM
                    rgdb."DbT5202NinteichosahyoGaikyoChosa" AS A1 
                  WHERE
                    T5202."shinseishoKanriNo" = A1."shinseishoKanriNo" 
                    AND T5202."ninteichosaRirekiNo" <![CDATA[<]]> A1."ninteichosaRirekiNo"
                ) 
              LEFT JOIN rgdb."DbT5910NinteichosaItakusakiJoho" T5910 
                ON T5201."ninteichosaItakusakiCode" = T5910."ninteichosaItakusakiCode" 
                AND T7051."shichosonCode" = T5910."shichosonCode" 
                AND T5910."jokyoFlag" = True 
              LEFT JOIN rgdb."DbT5913ChosainJoho" T5913 
                ON T5201."ninteichosaItakusakiCode" = T5913."ninteiChosaItakusakiCode" 
                AND T5201."ninteiChosainCode" = T5913."ninteiChosainCode" 
                AND T7051."shichosonCode" = T5913."shichosonCode" 
                AND T5913."jokyoFlag" = True 
              LEFT JOIN rgdb."DbT5301ShujiiIkenshoIraiJoho" AS T5301 
                ON T5101."shinseishoKanriNo" = T5301."shinseishoKanriNo" 
                AND T5301."logicalDeletedFlag" = False 
                AND NOT EXISTS ( 
                  SELECT
                    'X' 
                  FROM
                    rgdb."DbT5301ShujiiIkenshoIraiJoho" AS B2 
                  WHERE
                    T5301."shinseishoKanriNo" = B2."shinseishoKanriNo" 
                    AND B2."logicalDeletedFlag" = False 
                    AND T5301."ikenshoIraiRirekiNo" <![CDATA[<]]> B2."ikenshoIraiRirekiNo"
                ) 
              LEFT JOIN rgdb."DbT5302ShujiiIkenshoJoho" AS T5302 
                ON T5101."shinseishoKanriNo" = T5302."shinseishoKanriNo" 
                AND NOT EXISTS ( 
                  SELECT
                    'X' 
                  FROM
                    rgdb."DbT5302ShujiiIkenshoJoho" AS A2 
                  WHERE
                    T5302."shinseishoKanriNo" = A2."shinseishoKanriNo" 
                    AND T5302."ikenshoIraiRirekiNo" <![CDATA[<]]> A2."ikenshoIraiRirekiNo"
                ) 
              LEFT JOIN rgdb."DbT5911ShujiiIryoKikanJoho" T5911 
                ON T5301."shujiiIryokikanCode" = T5911."shujiiIryokikanCode" 
                AND T7051."shichosonCode" = T5911."shichosonCode" 
                AND T5911."jokyoFlag" = True 
              LEFT JOIN rgdb."DbT5912ShujiiJoho" T5912 
                ON T5301."shujiiIryokikanCode" = T5912."shujiiIryokikanCode" 
                AND T5301."shujiiCode" = T5912."shujiiCode" 
                AND T7051."shichosonCode" = T5912."shichosonCode" 
                AND T5912."jokyoFlag" = True 
              LEFT JOIN rgdb."DbT5123NinteiKeikakuJoho" T5123 
                ON T5101."shinseishoKanriNo" = T5123."shinseishoKanriNo" 
              LEFT JOIN rgdb."DbT5116IchijiHanteiKekkaJoho" T5116 
                ON T5101."shinseishoKanriNo" = T5116."shinseishoKanriNo" 
            WHERE
              T5101."hihokenshaNo" = #{被保険者番号} 
              AND T5101."logicalDeletedFlag" = FALSE 
            ORDER BY
              T5101."zenkaiNinteiYMD" DESC 
            LIMIT
              2
    </select>
