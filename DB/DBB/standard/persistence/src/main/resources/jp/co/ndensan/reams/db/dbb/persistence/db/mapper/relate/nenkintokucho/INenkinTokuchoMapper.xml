    <select resultOrdered="true" id="get取込ファイル_回付情報"  resultMap="resultMap_NenkinTokuchoEntity"  fetchSize="500">
        SELECT
        <include refid = "allColumns_TorikomiFileKaifuJohoTemp"/>,
        <include refid="allColumns_TorikomiFileTrailRecordTemp"/>
        FROM "TorikomiFileKaifuJohoTemp"
        INNER JOIN 
             "TorikomiFileTrailRecordTemp"
        ON
             "TorikomiFileKaifuJohoTemp"."recordKubun"='2'
        AND
             "TorikomiFileTrailRecordTemp"."recordKubun"='3'
        AND
             "TorikomiFileKaifuJohoTemp"."shichosoCode"="TorikomiFileTrailRecordTemp"."shichosoCode"
        AND
             "TorikomiFileKaifuJohoTemp"."tokuchoGimuCode"="TorikomiFileTrailRecordTemp"."tokuchoGimuCode"
        AND
             "TorikomiFileKaifuJohoTemp"."tsuchiNaiyoCode"="TorikomiFileTrailRecordTemp"."tsuchiNaiyoCode"
        ORDER BY
             "TorikomiFileTrailRecordTemp"."tsuchiNaiyoCode"       
    </select>
    
