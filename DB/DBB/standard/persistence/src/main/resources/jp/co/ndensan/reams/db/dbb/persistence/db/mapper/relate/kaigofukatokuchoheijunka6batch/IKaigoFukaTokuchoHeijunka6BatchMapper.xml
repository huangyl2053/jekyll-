    <select id="get対象外データ" resultOrdered="true" resultMap="resultMap_Taishogai" parameterType="jp.co.ndensan.reams.db.dbb.definition.mybatisprm.kaigofukatokuchoheijunka6batch.TokuchoHeijunkaRokuBatchTaishoParameter">
        SELECT
            対象外データTemp.*,
            "ShikibetsuTaisho".*,
            年金特徴回付情報."dtTokubetsuChoshuGimushaCode",
            徴収方法Newest."kariNenkinCode"
        FROM 
             "TmpTaishogai" AS 対象外データTemp
        INNER JOIN
            "DbV2001ChoshuHoho" AS 徴収方法Newest
        ON
            対象外データTemp."dbT2002Fuka_fukaNendo" = 徴収方法Newest."fukaNendo"
            AND 対象外データTemp."dbT2002Fuka_hihokenshaNo" = 徴収方法Newest."hihokenshaNo"
        LEFT JOIN
            rgdb."UeT0511NenkinTokuchoKaifuJoho" AS 年金特徴回付情報
        ON
            徴収方法Newest."kariNenkinNo" = 年金特徴回付情報."kisoNenkinNo"
            AND 徴収方法Newest."kariNenkinCode" = 年金特徴回付情報."nenkinCode"
            AND 徴収方法Newest."kariHosokuM" = 年金特徴回付情報."hosokuTsuki"
        INNER JOIN
            rgua."UaFt200FindShikibetsuTaisho"('','',TRUE,'DB',FALSE,'','','','','','','','','','','',true,
            '','','','','','','',true,'','','','','','','','','','','',true,true,true,true,true,true,true,true,true,true,
            true,'','','','','','','','',true,'','',true,true,true,'','','','','','','','','','','','','','','','','','','{}') AS "ShikibetsuTaisho"
        ON 
            対象外データTemp."dbT2002Fuka_shikibetsuCode" =  "ShikibetsuTaisho"."shikibetsuCode"
        WHERE
            対象外データTemp."dbT2002Fuka_choteiNendo" = #{調定年度}
            AND 対象外データTemp."dbT2002Fuka_fukaNendo" = #{賦課年度}
            AND 年金特徴回付情報."tsuchiNaiyoCode" in ('00', '30')
            AND 年金特徴回付情報."shoriNendo" = #{調定前年度}
        ${出力順}
    </select>
