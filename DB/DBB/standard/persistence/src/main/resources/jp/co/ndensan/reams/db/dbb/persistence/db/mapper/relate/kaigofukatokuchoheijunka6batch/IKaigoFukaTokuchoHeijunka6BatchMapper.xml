    <select id="get対象者データ" resultOrdered="true" resultMap="resultMap_Taishosha" parameterType="jp.co.ndensan.reams.db.dbb.definition.mybatisprm.kaigofukatokuchoheijunka6batch.TokuchoHeijunkaRokuBatchTaishoParameter">
        SELECT
            計算後情報.*,
            "ShikibetsuTaisho".*,
            年金特徴回付情報."dtTokubetsuChoshuGimushaCode"
        FROM 
            rgdb."DbT2015KeisangoJoho" AS 計算後情報
        LEFT JOIN
            rgdb."UeT0511NenkinTokuchoKaifuJoho" AS 年金特徴回付情報
        ON
            計算後情報."kariNenkinNo" = 年金特徴回付情報."kisoNenkinNo"
            AND 計算後情報."kariNenkinCode" = 年金特徴回付情報."nenkinCode"
            AND 計算後情報."kariHosokuM" = 年金特徴回付情報."hosokuTsuki"
        LEFT JOIN
            "TmpHeijunkaKeisanKeka" AS 平準化計算結果Temp
        ON
            計算後情報."tsuchishoNo" = 平準化計算結果Temp."tsuchishoNo"
        INNER JOIN
            rgua."UaFt200FindShikibetsuTaisho"('','',TRUE,'DB',FALSE,'','','','','','','','','','','',true,
            '','','','','','','',true,'','','','','','','','','','','',true,true,true,true,true,true,true,true,true,true,
            true,'','','','','','','','',true,'','',true,true,true,'','','','','','','','','','','','','','','','','','','{}') AS "ShikibetsuTaisho"
        ON 
            "ShikibetsuTaisho"."shikibetsuCode" = 計算後情報."shikibetsuCode" 
        WHERE
            計算後情報."choteiNendo" = #{調定年度}
            AND 計算後情報."fukaNendo" = #{賦課年度}
            AND (計算後情報."koseiZengoKubun" = '2' OR 計算後情報."koseiZengoKubun" = '1')
            AND 計算後情報."sakuseiShoriName" = '特徴平準化計算_6月分'
            AND 計算後情報."choteiNichiji" = #{調定日時}
            AND 年金特徴回付情報."tsuchiNaiyoCode" in ('00', '30')
            AND 年金特徴回付情報."shoriNendo" = #{調定前年度}
        ${出力順}
    </select>
