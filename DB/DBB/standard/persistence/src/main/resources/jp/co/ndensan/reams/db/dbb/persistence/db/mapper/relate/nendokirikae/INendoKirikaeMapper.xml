<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- @reamsid_L DBB-5722-030 xuhao -->

<mapper namespace="jp.co.ndensan.reams.db.dbb.persistence.db.mapper.relate.nendokirikae.INendoKirikaeMapper">
    <select resultOrdered="true" id="select処理日付管理マスタ"
            parameterType="jp.co.ndensan.reams.db.dbb.definition.mybatisprm.nendokirikae.NendoKirikaeMybatisParameter"
            resultMap = "jp.co.ndensan.reams.db.dbx.persistence.db.mapper.basic.IDbT7022ShoriDateKanriMapper.ResultMap_DbT7022ShoriDateKanriEntity" fetchSize="500">
        SELECT
        <include refid="jp.co.ndensan.reams.db.dbx.persistence.db.mapper.basic.IDbT7022ShoriDateKanriMapper.allColumns_DbT7022ShoriDateKanri" />
        FROM
        rgdb."DbT7022ShoriDateKanri"
        WHERE rgdb."DbT7022ShoriDateKanri"."nendo" = #{調定年度}
        AND   rgdb."DbT7022ShoriDateKanri"."subGyomuCode" = 'DBB'
    </select>

    <select resultOrdered="true" id="select徴収方法Newest"
            parameterType="jp.co.ndensan.reams.db.dbb.definition.mybatisprm.nendokirikae.NendoKirikaeMybatisParameter"
            resultMap="jp.co.ndensan.reams.db.dbx.persistence.db.mapper.basic.IDbV2001ChoshuHohoMapper.ResultMap_DbV2001ChoshuHohoEntity" fetchSize="500">
        SELECT
        <include refid="jp.co.ndensan.reams.db.dbx.persistence.db.mapper.basic.IDbV2001ChoshuHohoMapper.allColumns_DbV2001ChoshuHoho" />
        FROM
        rgdb."DbV2001ChoshuHoho"
        LEFT JOIN rgdb."DbV1001HihokenshaDaicho"
        ON rgdb."DbV2001ChoshuHoho"."hihokenshaNo" = rgdb."DbV1001HihokenshaDaicho"."hihokenshaNo"
        WHERE rgdb."DbV2001ChoshuHoho"."fukaNendo" = #{調定年度}
    </select>

    <select resultOrdered="true" id="select業務コンフィグ"
            resultMap="ResultMap_UzT0002CustomConfigEntity" fetchSize="500">
        SELECT *
        FROM
        rgdb."UzT0002CustomConfig"
        WHERE rgdb."UzT0002CustomConfig"."configName" = '日付関連_調定年度'
    </select>
    <resultMap id="ResultMap_UzT0002CustomConfigEntity" type="jp.co.ndensan.reams.uz.uza.util.config.entity.UzT0002CustomConfigEntity" autoMapping="true">
        <id property="schemaName" column="schemaName" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <id property="subGyomuCode" column="subGyomuCode" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.SubGyomuCodeTypeHandler"/>
        <id property="configName" column="configName" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <id property="insertTimestamp" column="insertTimestamp" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RDateTimeTypeHandler"/>
        <id property="lasdecCode" column="lasdecCode" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.LasdecCodeTypeHandler"/>
        <id property="optionKeyword" column="optionKeyword" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <id property="effectiveDate" column="effectiveDate" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RDateTypeHandler"/>
        <result property="insertDantaiCd" column="insertDantaiCd" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <result property="insertReamsLoginId" column="insertReamsLoginId" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <result property="insertContextId" column="insertContextId" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.UUIDTypeHandler"/>
        <result property="isDeleted" column="isDeleted" />
        <result property="updateCount" column="updateCount" />
        <result property="lastUpdateTimestamp" column="lastUpdateTimestamp" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RDateTimeTypeHandler"/>
        <result property="lastUpdateReamsLoginId" column="lastUpdateReamsLoginId" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <result property="configValue" column="configValue" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <result property="changeLog" column="changeLog" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.RStringTypeHandler"/>
        <result property="seq" column="seq" typeHandler="jp.co.ndensan.reams.uz.uza.util.db.typehandlers.BigSerialTypeHandler"/>
    </resultMap>

    <select resultOrdered="true" id="select処理日付管理マスタ更新"
            parameterType="jp.co.ndensan.reams.db.dbb.definition.mybatisprm.nendokirikae.NendoKirikaeMybatisParameter"
            resultMap="jp.co.ndensan.reams.db.dbx.persistence.db.mapper.basic.IDbT7022ShoriDateKanriMapper.ResultMap_DbT7022ShoriDateKanriEntity" fetchSize="500">
        SELECT
        <include refid="jp.co.ndensan.reams.db.dbx.persistence.db.mapper.basic.IDbT7022ShoriDateKanriMapper.allColumns_DbT7022ShoriDateKanri" />
        FROM
        rgdb."DbT7022ShoriDateKanri"
        WHERE rgdb."DbT7022ShoriDateKanri"."shoriName" = '年度切替'
        AND   rgdb."DbT7022ShoriDateKanri"."nendo" = #{調定年度}
    </select>

</mapper>
